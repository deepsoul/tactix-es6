{
  "name": "agency-boilerplate",
  "version": "4.0.0",
  "description": "agency - frontend boilerplate",
  "main": "none",
  "scripts": {
    "heroku": "NODE_ENV=production node node_modules/gulp/bin/gulp run --serverConfig=./env/config/server.json --gulpFileMap=./env/config/gulp-file-map.json --server=agency-server",
    "build": "NODE_ENV=build node node_modules/gulp/bin/gulp build --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/",
    "dev": "NODE_ENV=development node node_modules/gulp/bin/gulp run --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/ --server=agency-server",
    "dev-ip": "NODE_ENV=development node node_modules/gulp/bin/gulp run --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/ --server=agency-server --ip=true",
    "prod": "NODE_ENV=production node node_modules/gulp/bin/gulp run --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/ --server=agency-server",
    "server": "NODE_ENV=development node node_modules/gulp/bin/gulp default --serverConfig=/env/config/hapi/config --server=agency-server",
    "test": "npm run build",
    "template": "yo agency-boilerplate:hbs-template",
    "template-package": "yo agency-boilerplate:hbs-package-template",
    "create-package": "yo agency-boilerplate:package",
    "image-opt": "yo agency-boilerplate:image-opt",
    "svg-opt": "yo agency-boilerplate:svg-opt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StephanGerbeth/agency-boilerplate.git"
  },
  "keywords": [
    "boilerplate"
  ],
  "author": "Stephan Gerbeth",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/StephanGerbeth/agency-boilerplate/issues"
  },
  "homepage": "https://github.com/StephanGerbeth/agency-boilerplate#readme",
  "devDependencies": {
    "agency-environment": "git+https://git@github.com/StephanGerbeth/agency-environment.git",
    "agency-server": "git+https://git@github.com/StephanGerbeth/agency-server.git",
    "fb": "^1.0.2",
    "htmlescape": "^1.1.0",
    "jshint": "^2.8.0",
    "minimist": "^1.2.0"
  },
  "dependencies": {
    "ampersand-collection": "^2.0.0",
    "ampersand-model": "^8.0.0",
    "ampersand-state": "^5.0.2",
    "ampersand-view": "^10.0.1",
    "animejs": "^1.1.3",
    "blockadblock": "^3.2.0",
    "classlist-polyfill": "^1.0.3",
    "element-closest": "^2.0.2",
    "element-dataset": "^2.2.4",
    "enum": "^2.2.0",
    "es6-promise": "^3.0.2",
    "fg-loadcss": "^1.2.1",
    "firebase": "^4.1.1",
    "fontfaceobserver": "^1.6.1",
    "historyjs": "^1.8.0-b2",
    "jquery": "^3.1.1",
    "lodash": "^4.6.1",
    "modernizr": "^3.4.0",
    "outdated-browser": "^1.0.2",
    "pepjs": "^0.4.0",
    "prevent-overscroll": "^0.1.0",
    "purecss": "^0.6.0"
  }
}
